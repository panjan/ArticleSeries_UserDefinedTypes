uses "console"

type vector2d
  x as single
  y as single
end type

type vector3d extends vector2d
  z as single
end type

dim v as vector3d
printL "v:   " + varPtr(v)
printL "v.x: " + varPtr(v.x)
printL "v.y: " + varPtr(v.y)
printL "v.z: " + varPtr(v.z)

printL

single x, y, z
